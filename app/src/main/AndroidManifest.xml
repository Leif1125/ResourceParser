<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.dixon.resourceparser">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".core.pub.activity.BaseApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".func.home.view.MovieOutlineActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".func.search.view.SearchActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".func.special.view.SpecialActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".func.special.view.SpecialDetailActivity"></activity>
    </application>

</manifest>